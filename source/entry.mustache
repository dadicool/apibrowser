<li id="{{name}}">
	<h3>
		{{name}}
		{{#isFunction}}
			<span class="params">
				{{?params}}
					(<ul class="params">
					{{#params}}
						<li>{{name}}{{>type}}</li>
					{{/params}}
					</ul>)
				{{/params}}
				{{^params}}(){{/params}}
			</span>
			{{?returns}}
				<span class="returns">&#8594; {{returns}}</span>
			{{/returns}}
		{{/isFunction}}
		{{^isFunction}}
			<span class="returns">&#8594; {{type}}</span>
		{{/isFunction}}
	</h3>
	<div class="doc">{{&doc}}</div>
</li>
