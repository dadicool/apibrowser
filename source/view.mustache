
{{#constructor}}
<div id="heading">
	<h1>
		<span class="icon"></span>
		{{id}}
		<small>(
			{{#params}}
				<!-- if position trueish (!== 0) -->
				{{#position}} , {{/position}}
				<span class="param">{{*}}<sub>{{type}}</sub></span>
			{{/params}}
			<span class="param optional">crap<sub>String</sub></span>
		)</small>
	</h1>
</div>

<div class="triangle-l"></div>
<div class="triangle-r"></div>

<div class="meta">

<!--
	Implements <a href="#">Renderer</a>.
	Inherits from <a href="#">BaseRenderer</a>, <a href="#">ExtraClass</a> and <a href="#">Class</a>.
-->
</div>

<div id="description">
{{doc}}
</div>

{{/constructor}}


<div class="detail">

	<div class="line-background"></div>

	<h2>Statics<span>Returns</span></h2>
	<ul>
		{{#statics}}
		<li>
			<span class="line-number">{{line}}</span>
			<h3>
				{{.}}
				<span class="params">
	   			{{#params}}
	   				{{#position}} , {{/position}}
	   				<span class="type">{{type}}</span> {{.}}
	   			{{/params}}
				</span>
				<span class="return-value">{{returns}}</span>
			</h3>
		</li>
		{{/statics}}
		{{^statics}}
		<li>No Members</li>
		{{/statics}}
	</ul>

</div>

