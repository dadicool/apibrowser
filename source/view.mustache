
<div id="heading">
	<h1>
		<span class="icon"></span>
		{{id}}
		<small>
			{{#constructor}} (
			{{#params}}
	   			{{#position}} , {{/position}}
				<span class="param">{{name}}<sub>{{type}}</sub></span>
			{{/params}}
			) {{/constructor}}
		</small>
	</h1>
</div>

<div class="meta">
	Implements <a href="#">Renderer</a>.
	Inherits from <a href="#">BaseRenderer</a>, <a href="#">ExtraClass</a> and <a href="#">Class</a>.
</div>

<div id="description">
{{&main.doc}}
</div>

<h2>Statics</h2>
<ul>
	{{#statics}}
	<li data-hash=":{{name}}">
		<h3>
			{{name}}
			<span class="params">(
			{{#params}}
			{{#position}} , {{/position}}
				<span class="param">
					{{name}}
					<span class="type"> {{type}} </span>
				</span>
				{{/params}}
			)</span>
		</h3>
		<span class="return-value">{{returns}}{{^returns}}-{{/returns}}</span>
		<div class="doc">{{&doc}}</div>
	</li>
	{{/statics}}
</ul>

<h2>Members</h2>
<ul>
	{{#members}}
	<li data-hash=":{{name}}">
		<h3>
			{{name}}
			<span class="params">
				{{#params}}
				{{#position}} , {{/position}}
				<span class="type">{{type}}</span> {{.}}
				{{/params}}
			</span>
		</h3>
		<span class="return-value">{{returns}}{{^returns}}-{{/returns}}</span>
		<div class="doc">{{&doc}}</div>
	</li>
	{{/members}}
</ul>

