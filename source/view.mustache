
<div class="heading">
	<h1>{{id}}</h1>
</div>

<div class="meta">
</div>

<div class="description">
{{&main.doc}}
</div>



{{?statics}}
<div class="statics">
	<h2>Statics</h2>
	<ul>
		{{#statics}}
		<li data-hash=":{{name}}">
			<h3>
				{{name}}
				{{?params}}
					<span class="params">(
					{{#params}}
					{{#position}} , {{/position}}
						<span class="param">
							{{name}}
							<span class="type"> {{type}} </span>
						</span>
						{{/params}}
					)</span>
				{{/params}}
				{{?returns}}
					<span class="returns">-> {{returns}}</span>
				{{/returns}}
			</h3>
			<div class="doc">{{&doc}}</div>
		</li>
		{{/statics}}
	</ul>
</div>
{{/statics}}

{{#constructor}}
	<h2>Constructor</h2>
	<div class="constructor">
		{{id}} ({{#params}}
			{{#position}} , {{/position}}
			<span class="param">{{name}}<sub>{{type}}</sub></span>
		{{/params}})
	</div>
{{/constructor}}

{{?properties}}
	<div class="properties">
		<h2>Properties</h2>
		{{#properties}}
			<li>{{name}}</li>
		{{/properties}}
	</div>
{{/properties}}

{{?events}}
	<div class="events">
		<h2>Events</h2>
		{{#events}}
			<li>{{name}}</li>
		{{/events}}
	</div>
{{/events}}

{{?members}}
	<div class="members">
		<h2>Members</h2>
		<ul>
			{{#members}}
			<li data-hash=":{{name}}">
				<h3>
					{{name}}
					<span class="params">
						{{#params}}
						{{#position}} , {{/position}}
						<span class="type">{{type}}</span> {{.}}
						{{/params}}
					</span>
				</h3>
				<span class="return-value">{{returns}}{{^returns}}-{{/returns}}</span>
				<div class="doc">{{&doc}}</div>
			</li>
			{{/members}}
		</ul>
	</div>
{{/members}}

