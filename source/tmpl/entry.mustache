<h3>
	{{name}}
	{{?isFunction}}
		{{>params}}
	{{/isFunction}}
	{{>tags}}
	{{?isFunction}}
		{{?returns}}
			<span class="returns"> &#8594; {{returns}}</span>
		{{/returns}}
	{{/isFunction}}
	{{^isFunction}}
		{{?value}}
			<span class="returns"> &#8594; {{>type}}: <code>{{value}}</code></span>
		{{/value}}
		{{^value}}
			<span class="returns"> &#8594; {{>type}}</span>
		{{/value}}
	{{/isFunction}}
</h3>
<div class="details">
	<div class="doc">
		{{&doc}}
	</div>
	{{>info}}
	{{>origin}}
</div> 
