	<h3>
		{{name}}
		{{#isFunction}}
			<span class="params">
				{{?params}}
					(<ul class="params">
					{{#params}}
						<li>
							{{name}}{{>type}}
							{{?errornous}}<span class="errornous">&#9888;</span>{{/errornous}}
						</li>
					{{/params}}
					</ul>)
				{{/params}}
				{{^params}}(){{/params}}
			</span>
			{{?returns}}
				<span class="returns">&#8594; {{returns}}</span>
			{{/returns}}
		{{/isFunction}}
		{{^isFunction}}
			<span class="returns">&#8594; {{type}}</span>
			{{?errornous}}<span class="errornous">&#9888;</span>{{/errornous}}
		{{/isFunction}}
	</h3>
	<div class="doc">{{&doc}}</div>

