<h3>
	{{name}}
	{{?isFunction}}
		{{>params}}
	{{/isFunction}}
	{{>tags}}
	{{?isFunction}}
		{{?returns}}
			<span class="returns"> &#8594; <a href="#{{returns}}">{{returns}}</a></span>
		{{/returns}}
	{{/isFunction}}
	{{^isFunction}}
		{{?value}}
			<span class="returns"> [{{>type}}: <code>{{value}}</code>]</span>
		{{/value}}
		{{^value}}
			<span class="returns"> [{{>type}}]</span>
		{{/value}}
	{{/isFunction}}
	{{?summary}}
		<div class="summary">{{&summary}}</div>
	{{/summary}}
</h3>
<div class="details">
	<div class="doc">
		{{&doc}}
	</div>
	{{>info}}
	{{>origin}}
</div> 
