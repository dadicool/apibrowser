{{?origin}}
	<div class="origin">
		<strong>Included from: </strong>
		{{#origin}}
			<a href="#{{.}}">{{.}}</a>
		{{/origin}}
	</div>
{{/origin}}

{{?overridden}}
	<div class="overridden">
		<strong>Overridden from: </strong>
		{{#overridden}}
			<a href="#{{.}}">{{.}}</a>
		{{/overridden}}
	</div>
{{/overridden}}

{{?interface}}
	<div class="implemented">
		<strong>Defined by: </strong>
		<a href="#{{interface}}">{{interface}}</a>
	</div>
{{/interface}}